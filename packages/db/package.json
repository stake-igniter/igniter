{
  "name": "@igniter/db",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./connection": {
      "import": "./dist/src/connection.js",
      "require": "./dist/src/connection.js",
      "types": "./dist/src/connection.d.ts"
    },
    "./provider/connection": {
      "import": "./dist/src/provider/connection.js",
      "require": "./dist/src/provider/connection.js",
      "types": "./dist/src/provider/connection.d.ts"
    },
    "./provider/enums": {
      "import": "./dist/src/provider/schema/enums.js",
      "require": "./dist/src/provider/schema/enums.js",
      "types": "./dist/src/provider/schema/enums.d.ts"
    },
    "./provider/schema": {
      "import": "./dist/src/provider/schema/index.js",
      "require": "./dist/src/provider/schema/index.js",
      "types": "./dist/src/provider/schema/index.d.ts"
    },
    "./middleman/connection": {
      "import": "./dist/src/middleman/connection.js",
      "require": "./dist/src/middleman/connection.js",
      "types": "./dist/src/middleman/connection.d.ts"
    },
    "./middleman/enums": {
      "import": "./dist/src/middleman/schema/enums.js",
      "require": "./dist/src/middleman/schema/enums.js",
      "types": "./dist/src/middleman/schema/enums.d.ts"
    },
    "./middleman/schema": {
      "import": "./dist/src/middleman/schema/index.js",
      "require": "./dist/src/middleman/schema/index.js",
      "types": "./dist/src/middleman/schema/index.d.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc --project ./tsconfig.json && tsc-alias -p ./tsconfig.json",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@igniter/commons": "workspace:*",
    "@igniter/logger": "workspace:*",
    "@igniter/pocket": "workspace:*",
    "drizzle-orm": "0.44.1",
    "pg": "8.14.0"
  },
  "devDependencies": {
    "@igniter/eslint-config": "workspace:*",
    "@igniter/typescript-config": "workspace:*",
    "@types/pg": "^8.11.11",
    "tsc-alias": "^1.8.15"
  }
}
