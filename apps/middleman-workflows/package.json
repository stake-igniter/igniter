    "@temporalio/common": "1.11.7",
    "@temporalio/worker": "1.11.7",
    "@temporalio/workflow": "1.11.7",
    "dotenv": "16.4.7",
    "drizzle-orm": "0.39.1",
    "long": "5.3.1"
  },
  "devDependencies": {
    "@igniter/eslint-config": "workspace:*",
    "@igniter/typescript-config": "workspace:*",
    "drizzle-kit": "0.30.4",
    "npm-run-all": "4.1.5",
    "tsc-watch": "6.2.1"
